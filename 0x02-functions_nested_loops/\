#include <stdio.h>
#include "main.h"
#include <ctype.h>

/**
  * _islower -function that checks for lowercase character.
  *
  * Description: we used if statment to return 1 or 0.
  *
  * Return: return 0 for success.
  */

int _islower(int c)
{
	if (islower(c))
		return (1);
	else
		return (0);

	return (0);
}
